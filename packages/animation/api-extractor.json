{
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"projectFolder": ".",
	"mainEntryPointFilePath": "./.tsbuild/index.d.ts",
	"bundledPackages": [],
	"compiler": {
		"tsconfigFilePath": "./tsconfig.json"
	},
	"apiReport": {
		"enabled": true,
		"reportFolder": "./api/",
		"reportFileName": "api-report.api.md"
	},
	"docModel": {
		"enabled": false
	},
	"dtsRollup": {
		"enabled": false
	},
	"tsdocMetadata": {
		"enabled": false
	},
	"messages": {
		"compilerMessageReporting": {
			"default": {
				"logLevel": "warning"
			}
		},
		"extractorMessageReporting": {
			"default": {
				"logLevel": "warning"
			}
		},
		"tsdocMessageReporting": {
			"default": {
				"logLevel": "none"
			}
		}
	}
} 